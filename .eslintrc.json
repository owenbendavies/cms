{
  "env": {
    "browser": true
  },
  "extends": ["plugin:import/recommended", "airbnb", "prettier"],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        },
        "newlines-between": "always"
      }
    ],
    "import/no-default-export": "error",
    "import/prefer-default-export": "off",
    "react/prop-types": "off"
  }
}
